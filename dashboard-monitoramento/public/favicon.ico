<svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 64 64" width="64" height="64">
  <!-- Fundo circular com gradiente -->
  <defs>
    <linearGradient id="bgGradient" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#0f3460;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#0f0f23;stop-opacity:1" />
    </linearGradient>
    <linearGradient id="iconGradient" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#4CAF50;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#2E7D32;stop-opacity:1" />
    </linearGradient>
  </defs>
  
  <!-- Círculo de fundo -->
  <circle cx="32" cy="32" r="30" fill="url(#bgGradient)" stroke="#4CAF50" stroke-width="2"/>
  
  <!-- Ícone de dashboard/monitoramento -->
  <g fill="url(#iconGradient)">
    <!-- Tela principal -->
    <rect x="14" y="18" width="36" height="24" rx="2" fill="none" stroke="url(#iconGradient)" stroke-width="2"/>
    
    <!-- Gráfico de barras -->
    <rect x="18" y="32" width="3" height="6" fill="url(#iconGradient)"/>
    <rect x="23" y="28" width="3" height="10" fill="url(#iconGradient)"/>
    <rect x="28" y="30" width="3" height="8" fill="url(#iconGradient)"/>
    <rect x="33" y="26" width="3" height="12" fill="url(#iconGradient)"/>
    <rect x="38" y="29" width="3" height="9" fill="url(#iconGradient)"/>
    <rect x="43" y="31" width="3" height="7" fill="url(#iconGradient)"/>
    
    <!-- Indicador de status (ponto verde) -->
    <circle cx="46" cy="22" r="2" fill="#4CAF50"/>
    
    <!-- Linha de conexão -->
    <path d="M20 46 L44 46" stroke="url(#iconGradient)" stroke-width="2" stroke-linecap="round"/>
    
    <!-- Pontos de dados -->
    <circle cx="20" cy="46" r="1.5" fill="#4CAF50"/>
    <circle cx="32" cy="46" r="1.5" fill="#4CAF50"/>
    <circle cx="44" cy="46" r="1.5" fill="#4CAF50"/>
  </g>
</svg>